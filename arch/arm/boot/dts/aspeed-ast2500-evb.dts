 &mac1 {
 	status = "okay";
 
 	pinctrl-names = "default";
 	pinctrl-0 = <&pinctrl_rgmii2_default &pinctrl_mdio2_default>;
	/*Below is the default MAC address and it should be updated via u-boot*/
	mac-address = [00 03 73 00 00 00];

	phy-mode = "rgmii";
};

&i2c0 {
	status = "okay";
	//Airmax Conn B, CPU0 PIROM, CPU1 PIROM

	temp1@48 {
		compatible = "ti,tmp275";
		reg = <0x48>;
		status = "okay";
	};
	
	temp2@49 {
		compatible = "ti,tmp275";
		reg = <0x49>;
		status = "okay";
	};
		
	temp3@4a {
		compatible = "ti,tmp275";
		reg = <0x4a>;
		status = "okay";
	};
		
	temp4@4c {
		compatible = "ti,tmp275";
		reg = <0x4c>;
		status = "okay";
	};

	temp5@4f {
		compatible = "ti,tmp275";
		reg = <0x4f>;
		status = "okay";
	};
};

&i2c1 {
	status = "okay";

	tps53679@60 {
		compatible = "ti,tps53679";
		reg = <0x60>;
	};

	tps53679@62 {
		compatible = "ti,tps53679";
		reg = <0x62>;
	};
	
	tps53622@66 {
		compatible = "ti,tps53679";
		reg = <0x66>;
	};
	
	tps53622@68 {
		compatible = "ti,tps53679";
		reg = <0x68>;
	};

	tps53659@70 {
		compatible = "ti,tps53679";
		reg = <0x70>;
	};
		
	tps53659@72 {
		compatible = "ti,tps53679";
		reg = <0x72>;
	};
	
	tps53659@74 {
		compatible = "ti,tps53679";
		reg = <0x74>;
	};
	
	tps53659@75 {
		compatible = "ti,tps53679";
		reg = <0x75>;
