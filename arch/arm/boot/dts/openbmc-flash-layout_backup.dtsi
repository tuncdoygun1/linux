partitions {
	compatible = "fixed-partitions";
	#address-cells = <1>;
	#size-cells = <1>;

	u-boot_backup@0 {
		reg = <0x0 0x60000>;
		label = "u-boot_backup";
	};

	u-boot-env_backup@60000 {
		reg = <0x60000 0x1000>;
		label = "u-boot-env_backup";
	};

	kernel_backup@61000 {
		reg = <0x61000 0x4BD000>;
		label = "kernel_backup";
	};

	rofs_backup@51E000 {
		reg = <0x51E000 0x1740000>;
		label = "rofs_backup";
	};

	rwfs_backup@1C5E000 {
		reg = <0x1C5E000 0x395000>;
		label = "rwfs_backup";
	};

	dtb_backup@1FF3000 {
		reg = <0x1FF3000 0xD000>;
		label = "dtb_backup";
	};
};